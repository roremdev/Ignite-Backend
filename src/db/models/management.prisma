model Management {
  id          Int         @id @default(autoincrement())
  name        String
  email       String
  password    String
  permissions Permissions
  createdAt   DateTime    @default(now()) @map("created_at")
  updateAt    DateTime    @updatedAt @map("updated_at")

  @@map("management")
}

enum Permissions {
  OWNER         @map("owner")
  ADMINISTRATOR @map("administrator")
  EDITOR        @map("editor")
  ANALYZER      @map("analyzer")
}
